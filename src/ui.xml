<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkAdjustment" id="MeanX">
    <property name="lower">-10</property>
    <property name="upper">10</property>
    <property name="step-increment">1</property>
    <property name="page-increment">10</property>
  </object>
  <object class="GtkAdjustment" id="MeanXAdjustment">
    <property name="lower">-10</property>
    <property name="upper">10</property>
    <property name="step-increment">1</property>
    <property name="page-increment">10</property>
  </object>
  <object class="GtkAdjustment" id="MeanY">
    <property name="lower">-10</property>
    <property name="upper">10</property>
    <property name="step-increment">1</property>
    <property name="page-increment">10</property>
  </object>
  <object class="GtkAdjustment" id="MeanYAdjustment1">
    <property name="lower">-10</property>
    <property name="upper">10</property>
    <property name="step-increment">1</property>
    <property name="page-increment">10</property>
  </object>
  <object class="GtkAdjustment" id="Pitch">
    <property name="lower">-3.14</property>
    <property name="upper">3.14</property>
    <property name="value">0.5</property>
    <property name="step-increment">0.01</property>
    <property name="page-increment">10</property>
  </object>
  <object class="GtkAdjustment" id="SDX">
    <property name="upper">10</property>
    <property name="value">2</property>
    <property name="step-increment">1</property>
    <property name="page-increment">10</property>
  </object>
  <object class="GtkAdjustment" id="SDY">
    <property name="upper">10</property>
    <property name="value">5</property>
    <property name="step-increment">1</property>
    <property name="page-increment">10</property>
  </object>
  <object class="GtkAdjustment" id="Yaw">
    <property name="upper">3.14</property>
    <property name="value">0.73</property>
    <property name="step-increment">0.01</property>
    <property name="page-increment">10</property>
  </object>
  <object class="GtkApplicationWindow" id="MainWindow">
    <property name="title">Gaussian PDF Plotter</property>
    <property name="child">
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkGrid">
            <property name="column-homogeneous">1</property>
            <child>
              <object class="GtkLabel">
                <property name="label" translatable="1">Mean X </property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="label" translatable="1">Mean Y</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="label" translatable="1">SD X</property>
                <layout>
                  <property name="column">2</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="label" translatable="1">SD Y</property>
                <layout>
                  <property name="column">2</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkScale" id="MeanXScale">
                <property name="draw-value">1</property>
                <property name="focusable">1</property>
                <property name="adjustment">MeanX</property>
                <property name="round-digits">1</property>
                <property name="digits">4</property>
                <property name="value-pos">left</property>
                <layout>
                  <property name="column">1</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkScale" id="MeanYScale">
                <property name="draw-value">1</property>
                <property name="focusable">1</property>
                <property name="adjustment">MeanY</property>
                <property name="round-digits">1</property>
                <property name="digits">4</property>
                <property name="value-pos">left</property>
                <layout>
                  <property name="column">1</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkScale" id="SDXScale">
                <property name="draw-value">1</property>
                <property name="focusable">1</property>
                <property name="adjustment">SDX</property>
                <property name="round-digits">1</property>
                <property name="digits">4</property>
                <property name="value-pos">left</property>
                <layout>
                  <property name="column">3</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkScale" id="SDYScale">
                <property name="draw-value">1</property>
                <property name="focusable">1</property>
                <property name="adjustment">SDY</property>
                <property name="round-digits">1</property>
                <property name="digits">4</property>
                <property name="value-pos">left</property>
                <layout>
                  <property name="column">3</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="label" translatable="1">Pitch</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="label" translatable="1">Roll</property>
                <property name="use-underline">1</property>
                <layout>
                  <property name="column">2</property>
                  <property name="row">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkScale" id="PitchScale">
                <property name="draw-value">1</property>
                <property name="focusable">1</property>
                <property name="adjustment">Pitch</property>
                <property name="round-digits">1</property>
                <property name="digits">4</property>
                <property name="value-pos">left</property>
                <layout>
                  <property name="column">1</property>
                  <property name="row">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkScale" id="YawScale">
                <property name="draw-value">1</property>
                <property name="focusable">1</property>
                <property name="adjustment">Yaw</property>
                <property name="round-digits">1</property>
                <property name="digits">4</property>
                <property name="value-pos">left</property>
                <layout>
                  <property name="column">3</property>
                  <property name="row">2</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkDrawingArea" id="MainDrawingArea">
            <property name="vexpand">1</property>
            <property name="width-request">600</property>
            <property name="height-request">600</property>
          </object>
        </child>
      </object>
    </property>
  </object>
</interface>
